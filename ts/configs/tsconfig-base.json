{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": ["es2015"],
    "moduleResolution": "node",
    "newLine": "LF",
    "noEmitOnError": false,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "pretty": true,
    "strict": true,
    "stripInternal": true,
    "target": "es5",
    "sourceMap": true
  }
}
